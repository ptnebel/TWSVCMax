<style>
        
        .Logo1{
                float:left;
                margin: 0;
                padding-top:0px;
                padding-left:0px;
                padding-right:0px;
                overflow: hidden;
                width: 319px;
                /*height:44px;*/
            }
            .Logoheader{
                  width:319px;
                  height:44px;
            }
            .buttonNormalStateSelectedForGroup a.buttonLinkForGroup{
                outline:0;
            }
            .buttonNormalTextWithIcon{
                padding-top: 5px;
                height: auto;
                width:117px;
                text-decoration:none;
            }
            a.buttonLink, a.buttonLink:hover {text-decoration:none;}
            
            #outer {display: table; height: 48px; padding:0}
            #middle {display: table-cell; vertical-align: middle;}
            #inner {word-wrap: break-word; width:117px;}
            .buttonNormalTextWithIconGroup {
