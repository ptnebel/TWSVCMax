                                        <apex:outputtext value="{!TAG006}" style="font-Size:12px;" />
                                    </apex:outputPanel>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection columns="1">
                                    <apex:outputPanel layout="block" styleClass="requiredInput">
