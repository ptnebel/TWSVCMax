                                <apex:outputPanel layout="block" styleClass="requiredInput">
                                    <apex:inputText style="width:200px;" value="{!POLineExp.SVMXC__Name__c}" disabled="{!(POLineExp.SVMXC__IsStandard__c || disabled)}"/>
                                    <apex:outputPanel layout="block" styleClass="requiredBlock"/>
