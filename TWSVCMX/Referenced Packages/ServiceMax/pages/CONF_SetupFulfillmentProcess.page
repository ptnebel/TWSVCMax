                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem datastyle="width:26%;">
                                <apex:outputPanel layout="block" styleClass="requiredInput">
                                    <apex:outputPanel layout="block" styleClass="requiredBlock"/>
                                    <apex:inputText value="{!POExp.SVMXC__Name__c}" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}" style="width:80%;"/>
                                </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem datastyle="width:24%;">
                                <apex:outputLabel value="{!TAG021}" style="padding-left:2.1%;font-weight:bold;font-size:12px;display:Block;Padding-Top:4px"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem datastyle="width:26%;">
                                <apex:outputPanel layout="block" styleClass="requiredInput">
                                    <apex:outputPanel layout="block" styleClass="requiredBlock"/>
                                    <apex:inputText value="{!POExp.SVMXC__ProcessID__c}" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}" style="width:80%;"/>
                                </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="1" rendered="{!renderPOExprBlock}" id="POExpTable">
                            <apex:actionStatus id="status3" startText="{!TAG032}"/>
                            <apex:pageBlockTable id="table1" value="{!LstPOCriteria}" var="item" style="width:96%;margin-left:1.1%">
                                <apex:column style="font-Size:12px;width:2%;text-align:center;">
                                    <apex:facet name="header"> </apex:facet>
                                    <apex:outputText value="{!Round(item.SVMXC__Sequence__c, 0)}"/>
                                </apex:column>
                                <apex:column style="font-Size:12px;width:20%;">
                                    <apex:facet name="header">{!TAG023}</apex:facet>
                                    <apex:SelectList style="width:80%;" size="1" value="{!item.SVMXC__Field_Name__c}" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}">
