                                </apex:column>
                                <apex:column style="font-Size:12px;width:10%;text-align:right;">
                                    <apex:commandButton image="{!URLFOR($Resource.SVMXC__COMM_Resources3, 'Resources/images/delete_exp_img.JPG')}" style="width:20px;height:18px;background:lightblue;" value="table2DeleteRow" title="{!TAG083}" action="{!POCriteria_DelRow}" status="status3" rerender="POExpTable" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}">
                                        <apex:param name="selectedrow2" value="{!Round(item.SVMXC__Sequence__c, 0)}" assignTo="{!SelectedRow}"/>
                                    </apex:commandButton>
                                    <apex:commandButton image="{!URLFOR($Resource.SVMXC__COMM_Resources3, 'Resources/images/add_exp_img.JPG')}" style="width:20px;height:18px;background:lightblue" value="table2AddRow" title="{!TAG082}" action="{!POCriteria_AddRow}" rerender="POExpTable" status="status3" rendered="{!if((POCriteriaSize == Round(item.SVMXC__Sequence__c, 0)) ,true,false)}" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}"/>
                                </apex:column>
                            </apex:pageBlockTable>
                            <apex:commandButton image="{!URLFOR($Resource.SVMXC__COMM_Resources3, 'Resources/images/add_exp_img.JPG')}" style="width:20px;height:18px;background:lightblue" value="table2AddRow" title="{!TAG082}" action="{!POCriteria_AddRow}" rerender="POExpTable" status="status3" rendered="{!if((POCriteriaSize == 0) ,true,false)}" disabled="{!(POExp.SVMXC__IsStandard__c || disabled)}"/>
                        </apex:pageBlockSection>
                        
                        <apex:pageblockSection columns="1" rendered="{!renderPOExprBlock}">
                            <apex:pageBlockSectionItem datastyle="width:25%;">
                                <apex:outputText value="{!TAG026}"  style="padding-left:1.3%;font-weight:bold;font-Size:12px;"/> 
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem datastyle="width:25%;padding-left:2%;">
