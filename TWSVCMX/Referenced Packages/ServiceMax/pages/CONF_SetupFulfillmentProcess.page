                                    </apex:SelectList>
                                </apex:column>
                                <apex:column style="font-Size:12px;width:10%;">
                                    <apex:facet name="header">{!TAG025}</apex:facet>
                                    <apex:inputText size="20" value="{!item.SVMXC__Operand__c}" disabled="{!(POLineExp.SVMXC__IsStandard__c || disabled)}"/>
