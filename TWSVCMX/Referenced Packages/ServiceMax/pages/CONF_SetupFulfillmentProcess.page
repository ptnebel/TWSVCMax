                                        <apex:inputText value="{!FulfillmentProcess.SVMXC__ProcessID__c}" style="width:500px;"/>
                                    </apex:outputPanel>
                                </apex:pageBlockSection>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem >
