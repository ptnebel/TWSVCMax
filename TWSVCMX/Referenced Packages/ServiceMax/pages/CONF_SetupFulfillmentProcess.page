                                        <apex:outputtext value="{!TAG008}" style="font-Size:12px;" />
                                    </apex:outputPanel>
                                </apex:pageBlockSection>
                                <apex:pageBlockSection columns="1">
                                    <apex:inputTextArea value="{!FulfillmentProcess.SVMXC__Description__c}" style="width:500px;"/>
                                </apex:pageBlockSection>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="1">
                            <apex:pageBlockSectionItem dataStyle="text-align:center;" datastyleClass="disableTab">
                                <apex:outputText value="{!TAG009}" style="font-weight: bold;font-Size:12px;color:#000000;" styleclass="disableTab"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="1">
                            <apex:pageBlockSectionItem >
                                <apex:pageBlockSection columns="1">
                                    <apex:outputtext value="{!TAG010}" style="font-Size:12px;"/>
                                </apex:pageBlockSection>
                                
                                <apex:pageBlockSection columns="1">
                                    <apex:outputPanel layout="block" styleClass="requiredInput">
                                        <apex:selectList value="{!selectedDoc1}" style="width:50%;" size="1">
                                            <apex:selectOptions value="{!lstDocumentFields}"/>
                                        </apex:selectList>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="block" styleClass="requiredInput">
                                        <apex:selectList value="{!selectedDoc2}" style="width:50%;" size="1">
                                            <apex:selectOptions value="{!lstDocumentFields}"/>
                                        </apex:selectList>
                                    </apex:outputPanel>

                                    <apex:outputPanel layout="block" styleClass="requiredInput">
                                        <apex:selectList value="{!selectedDoc3}" style="width:50%;" size="1">
                                            <apex:selectOptions value="{!lstDocumentFields}"/>
                                        </apex:selectList>
                                    </apex:outputPanel>
                                </apex:pageBlockSection>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem labelStyle="text-align:left;margin-left:0px;width:25%;">
                                <apex:pageBlockSection columns="1">
                                    <apex:outputtext value="{!TAG011}" style="font-Size:12px;width:100%;"/>
                                </apex:pageBlockSection>
                            
                                <apex:pageBlockSection columns="1">
                                    <apex:outputPanel layout="block" styleClass="requiredInput">
                                        <apex:selectList style="width:50%;" value="{!selectedProd1}" size="1">
                                            <apex:SelectOptions value="{!lstProductFields}"/>
                                        </apex:selectList>
                                    </apex:outputPanel>
                                    
                                    <apex:outputPanel layout="block" styleClass="requiredInput">
                                        <apex:selectList style="width:50%;" value="{!selectedProd2}" size="1">
                                            <apex:SelectOptions value="{!lstProductFields}"/>
                                        </apex:selectList>
                                    </apex:outputPanel>
                                </apex:pageBlockSection>
                            </apex:pageBlockSectionItem>

                            <!-- <apex:pageBlockSectionItem labelStyle="text-align:left;margin-left:0px;width:25%;">
                                <apex:pageBlockSection columns="1">
                                    <apex:outputtext value="{!TAG012}" style="font-Size:12px;width:100%;"/>
                                </apex:pageBlockSection>
                                
                                <apex:pageBlockSection columns="1">
                                    
                                </apex:pageBlockSection>
                            </apex:pageBlockSectionItem> -->
                        </apex:pageBlockSection>
                        
                        <apex:pageblockSection title="" collapsible="false"/>
                        <apex:pageblockSection columns="1">
                            <apex:pageBlockSectionItem datastyle="text-align:right;">
                                <apex:CommandButton value="{!TAG015} >>" status="status1" action="{!gotoCriteriaMappingsTab}" rerender="SVMX_FORM" immediate="true"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageblockSection>
                    </apex:pageblock>
                </apex:tab>
            <!--------------------------------------------------------------------- Tab-2 Starts Here------------------------------------------------------->
                <apex:tab id="criteriaMapping" label="{!TAG015}">
                    <apex:pageblock id="tab2PgBlock" mode="edit">
                        <apex:actionFunction id="OnChangePOExpr" name="OnChangePOExpr" action="{!OnChangePOExpr}" status="status2" rerender="tab2PgBlock"/>
                        <apex:actionFunction id="ViewEditPOCriteria" name="ViewEditPOCriteria" action="{!ViewEditPOCriteria}" status="status2" rerender="tab2PgBlock"/>
                        <apex:actionFunction id="CreateNewPOCriteria" name="CreateNewPOCriteria" action="{!CreateNewPOCriteria}" status="status2" rerender="tab2PgBlock"/>
                        
                        <apex:actionFunction id="OnChangePOLineExpr" name="OnChangePOLineExpr" action="{!OnChangePOLineExpr}" status="status4" rerender="tab2PgBlock"/>
                        <apex:actionFunction id="ViewEditPOLineCriteria" name="ViewEditPOLineCriteria" action="{!ViewEditPOLineCriteria}" status="status4" rerender="tab2PgBlock"/>
                        <apex:actionFunction id="CreateNewPOLineCriteria" name="CreateNewPOLineCriteria" action="{!CreateNewPOLineCriteria}" status="status4" rerender="tab2PgBlock"/>
                        
                        <apex:actionStatus id="status2" startText="{!TAG032}"/>
                        <apex:pageBlockSection columns="1">
                            <apex:pageBlockSectionItem dataStyle="margin-left:22%;left:22%;padding-left:22%;" datastyleClass="disableTab">
                                <apex:outputText value="{!TAG016}" style="font-weight: bold;font-Size:12px;color:#000000;"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="2" id="expBlockSec1">   
                            <apex:pageBlockSectionItem >
                                <apex:pageBlockSection columns="1">
                                    <apex:outputtext value="{!TAG017}" style="font-weight:bold;font-size:12px;"/>
                                </apex:pageBlockSection>
                            
