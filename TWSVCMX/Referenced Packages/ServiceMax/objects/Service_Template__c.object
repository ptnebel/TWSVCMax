    <validationRules>
        <fullName>Expenses_Duration_UoT_Check</fullName>
        <active>true</active>
        <description>If Expenses coverage % is specified, duration and unit of time should be specified too.</description>
        <errorConditionFormula>AND(NOT ISNULL( Expenses_Covered__c ), OR( ISNULL(  Duration_of_Expenses__c ), ISPICKVAL(  Unit_of_Time_Expenses__c , &quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>Unit_of_Time_Expenses__c</errorDisplayField>
        <errorMessage>Please specify the duration and unit of time of Expenses coverage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Labor_Duration_UoT_Check</fullName>
        <active>true</active>
        <description>If Labor coverage % is specified, duration and unit of time should be specified too.</description>
        <errorConditionFormula>AND(NOT ISNULL( Time_Covered__c ), OR( ISNULL(  Duration_of_Time_Coverage__c ), ISPICKVAL(  Unit_of_Time__c, &quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>Unit_of_Time__c</errorDisplayField>
        <errorMessage>Please specify the duration and unit of time of Labor coverage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Material_Duration_UoT_Check</fullName>
        <active>true</active>
        <description>If Material coverage % is specified, duration and unit of time should be specified too.</description>
        <errorConditionFormula>AND(NOT ISNULL(Material_Covered__c), OR( ISNULL( Duration_of_Material_Coverage__c ), ISPICKVAL( Unit_of_Time_Material__c, &quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>Unit_of_Time_Material__c</errorDisplayField>
        <errorMessage>Please specify the duration and unit of time of Material coverage.</errorMessage>
    </validationRules>
